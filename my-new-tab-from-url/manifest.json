{
  "manifest_version": 3,
  "name": "My New Tab",
  "version": "1.0",
  "description": "Заменяет стартовую страницу новой вкладки на 'My New Tab'.",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["activeTab"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' http://zzzzzz http://icon;"
  }
}
